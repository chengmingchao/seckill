<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cmc.seckill.server.dao.ItemDao">
    <select id="findItemList" resultType="com.cmc.seckill.entity.Item">
        select * from item
    </select>
    <select id="findItemById" resultType="com.cmc.seckill.entity.Item">
    select * from item where id=#{id}
    </select>

</mapper>
